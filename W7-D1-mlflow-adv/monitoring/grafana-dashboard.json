{
  "title": "W7 Inference Overview",
  "panels": [
    {
      "type": "timeseries",
      "title": "p95 latency (s)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(inference_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": {"x":0,"y":0,"w":12,"h":8}
    },
    {
      "type": "timeseries",
      "title": "Error rate",
      "targets": [
        {
          "expr": "sum(rate(inference_requests_total{code!=\"200\"}[5m])) / sum(rate(inference_requests_total[5m]))",
          "legendFormat": "err_rate"
        }
      ],
      "gridPos": {"x":12,"y":0,"w":12,"h":8}
    },
    {
      "type": "stat",
      "title": "Requests (5m)",
      "targets": [
        {
          "expr": "sum(rate(inference_requests_total[5m]))",
          "legendFormat": "rps"
        }
      ],
      "gridPos": {"x":0,"y":8,"w":6,"h":6}
    }
  ],
  "schemaVersion": 38,
  "version": 1
}

